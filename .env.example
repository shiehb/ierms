# Example environment variables for IERMS
# Copy this file to `.env` and fill in real values (DO NOT commit secrets)

# Django
DJANGO_SECRET_KEY=
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1
FRONTEND_URL=https://iermsdeploy.vercel.app

# Railway MySQL variables (Railway provides these names)
MYSQL_DATABASE=railway
MYSQLUSER=root
MYSQL_PASSWORD=
MYSQLPASSWORD=
MYSQLHOST=mysql.railway.internal
MYSQL_PORT=3306
MYSQLPORT=3306
MYSQL_URL=
MYSQL_PUBLIC_URL=

# Email (optional)
EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=
DEFAULT_FROM_EMAIL=

# Celery / Redis (optional)
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
